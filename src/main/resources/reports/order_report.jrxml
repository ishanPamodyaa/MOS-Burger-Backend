        <query language="SQL"><![CDATA[SELECT
    o.order_id,
    o.order_date,
    o.customer_id,
    o.total_price,
    o.bill_value,
    o.total_discount,
    od.product_product_id,
    od.order_qty,
    od.price,
    od.discount
FROM
    orders o
JOIN
    order_detail od ON o.order_id = od.order_order_id
WHERE
    o.order_id = $P{order_id}
]]></query>
